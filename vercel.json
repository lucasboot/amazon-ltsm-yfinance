{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "LOG_LEVEL": "INFO",
    "MODEL_VERSION": "1.0",
    "LOOKBACK": "60",
    "FEATURES": "open,high,low,close,volume",
    "TARGET": "close"
  },
  "functions": {
    "api/index.py": {
      "maxDuration": 10,
      "memory": 1024
    }
  }
}