{
  "version": 2,
  "buildCommand": "echo 'No build needed'",
  "outputDirectory": ".",
  "installCommand": "pip install -r requirements.txt",
  "framework": null,
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "LOG_LEVEL": "INFO",
    "MODEL_VERSION": "1.0",
    "LOOKBACK": "60",
    "FEATURES": "open,high,low,close,volume",
    "TARGET": "close"
  },
  "functions": {
    "api/index.py": {
      "runtime": "python3.9",
      "maxDuration": 60,
      "memory": 3008
    }
  }
}